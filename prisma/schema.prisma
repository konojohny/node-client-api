generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  // Correto: use a vari√°vel de ambiente DATABASE_URL
}

model Clients {
  id        Int      @id @default(autoincrement())
  name      String
  cpf       String
  dateBirth DateTime

  @@map("Clients")
}
